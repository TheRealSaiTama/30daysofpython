{
  "routes": [
    {
      "src": "/api/subscribe",
      "dest": "/src/api/subscribe.ts"
    },
    {
      "src": "/api/process-daily-tips",
      "dest": "/src/api/process-daily-tips.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/process-daily-tips",
      "schedule": "0 8 * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/process-daily-tips",
      "headers": [
        {
          "key": "x-api-key",
          "value": "${ADMIN_API_KEY}"
        }
      ]
    }
  ]
}